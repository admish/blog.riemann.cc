@font-face {
  font-family: 'Bitter';
  font-style: normal;
  font-weight: 400;
  src: url('bitter/bitter-regular-webfont.eot');
  src: local('Bitter-Regular'), local('Bitter Regular'), url('<%= settings.http_prefix + settings.fonts_dir %>/bitter/bitter-regular-webfont.eot?#iefix') format('embedded-opentype'), url('<%= settings.http_prefix + settings.fonts_dir %>/bitter/bitter-regular-webfont.woff') format('woff'), url('<%= settings.http_prefix + settings.fonts_dir %>/bitter/bitter-regular-webfont.ttf') format('truetype'), url('<%= settings.http_prefix + settings.fonts_dir %>/bitter/bitter-regular-webfont.svg#webfont') format('svg');
}
